<div class="col-md-6 col-md-offset-3" id="record-table">
	<table class="table table-striped table-bordered table-hover record-detail" id="recordtable">
		<thead>
			<tr>
				<th width="6%">ID</th>
				<th>OS</th>
				<th>Browser</th>
				<th>Time Created</th>
			</tr>
		</thead>
		<tbody>
		<% users.each do |u| %>
			<tr>
				<td><%= link_to "#{u.id}", user_path(u.id) %></td>
				<td><%= u.operating_system %></td>
				<td><%= u.browser %></td>
				<td><%= u.created_at.localtime %></td>
			</tr>
		<% end %>
		</tbody>
	</table>
</div>
