<div id="record-table">
	<table class="table table-striped table-bordered table-hover record-detail">
		<thead>
			<th>Record</th>
			<th>OS</th>
			<th>Browser</th>
		</thead>
		<% users.each do |u| %>
		<tr>
			<td><%= link_to "#{u.id}", user_path(u.id) %></td>
			<td><%= u.operating_system %></td>
			<td><%= u.browser %></td>
		</tr>
		<% end %>
	</table>
</div>
