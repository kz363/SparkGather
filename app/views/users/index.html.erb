
<div class="col-md-6 col-md-offset-3">
	<table class="table table-striped table-bordered">
		<tbody>
			<tr>
				<th>Company</th>
				<th># of Users</th>
				<th>Last Updated</th>
			</tr>
			<% @company_info.each do |company_info| %>
			<tr>
				<td><a href="/users?c=<%=company_info[0]%>"><%=company_info[0]%></a></td>
				<td><%=company_info[1]%></td>
				<td><%=company_info[2]%></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
